	stack ms;
	int pjgkata,hasil;
	string r,p;
	cout<<"-------------IMPLEMENTASI BENTUK INFIX KE POSTFIX-------------";
	cout<<"\n\nBentuk Infix : ";
	getline(cin,r);
	pjgkata=r.length();
	for (int i=0;i<pjgkata;i++){
		if (r[i] >= '0' && r[i] <= '9'){
